<i-cell-group>

    <i-cell title="增加活动" is-link bindtap="goToaddactivity">
      <i-grid-icon  slot="icon">
            <image src="/images/add.png" />
      </i-grid-icon>
    </i-cell>

      <i-cell title="删除活动" is-link bindtap="goTodeleteactivity">
      <i-grid-icon  slot="icon">
            <image src="/images/delete.png" />
      </i-grid-icon>
    </i-cell>
    <i-cell title="增加班次" is-link bindtap="goToaddTurn">
      <i-grid-icon  slot="icon">
            <image src="/images/add.png" />
      </i-grid-icon>
    </i-cell>

      <i-cell title="删除班次" is-link bindtap="goTodeleteTurn">
      <i-grid-icon  slot="icon">
            <image src="/images/delete.png" />
      </i-grid-icon>
    </i-cell>



      <i-cell title="导出名单" is-link bindtap="goToexcel">
      <i-grid-icon  slot="icon">
            <image src="/images/mingdan.png" />
      </i-grid-icon>
    </i-cell>
   

         <i-cell title="添加管理员" is-link bindtap="goToAddAdmin">
      <i-grid-icon  slot="icon">
            <image src="/images/mingdan.png" />
      </i-grid-icon>
    </i-cell>

             <i-cell title="放弃管理员身份(再次成为管理员需其他管理员添加）" is-link bindtap="Quit">
      <i-grid-icon  slot="icon">
            <image src="/images/mingdan.png" />
      </i-grid-icon>
    </i-cell>
</i-cell-group>