
<view class="news-list">
  <view class="news-item" wx:for="{{myTurn}}" wx:key="{{item.turn_id}}" >
   <view class="rongqi">
    <view  >
        <view class="title"> 站点：{{item.placeName}}</view>
        <view class="time">活动时间：{{item.begin_time}}--{{item.end_time}}</view>
        <view class="time">截止报名时间:活动前一天</view>
        <button  bindtap="quit" data-id="{{item.turn_id}}">退选</button>
    </view>
    <view class="fuzhu">
        <ul>
          <li>
             <image class='suo' src="/images/user.png"></image> 
          </li>
          <li class="person">
             <text >{{item.attendNum}}/{{item.limit}}</text>
          </li>
        </ul>
    </view>
    
    </view>
  </view>
</view>
